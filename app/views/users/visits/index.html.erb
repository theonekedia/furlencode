<p id="notice"><%= notice %></p>

<h1>Listing Users Visits</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_visits.each do |users_visit| %>
      <tr>
        <td><%= users_visit.date %></td>
        <td><%= users_visit.status %></td>
        <td><%= link_to 'Show', users_visit %></td>
        <td><%= link_to 'Edit', edit_users_visit_path(users_visit) %></td>
        <td><%= link_to 'Destroy', users_visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Visit', new_users_visit_path %>
