<%= simple_form_for(@trackers_event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tracker_id %>
    <%= f.input :event_name %>
    <%= f.input :value %>
    <%= f.input :referrer %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
