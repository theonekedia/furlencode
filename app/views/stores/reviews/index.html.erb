<p id="notice"><%= notice %></p>

<h1>Listing Stores Reviews</h1>

<table class= table table-bordered>
  <thead>
    <tr>
      <th>Liked</th>
      <th>Critic</th>
      <th>User</th>
      <th>Store</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @stores_reviews.each do |stores_review| %>
      <tr>
        <td><%= stores_review.liked %></td>
        <td><%= stores_review.critic %></td>
        <td><%= stores_review.user_id %></td>
        <td><%= stores_review.store_id %></td>
        <td><%= link_to 'Show', stores_review %></td>
        <td><%= link_to 'Edit', edit_stores_review_path(stores_review) %></td>
        <td><%= link_to 'Destroy', stores_review, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Review', new_stores_review_path %>
